{
    "env": {
        "commonjs": true,
        "es2021": true,
        "browser": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-console": "off",
      "no-underscore-dangle": "off",
      "no-unused-vars": "error",
      "import/extensions": ["error", "never", { "mjs": "always" }]
    }
}
